{
  "name": "scriptloader-component",
  "version": "0.0.1",
  "description": "A React Component for reacting to scripts loading.",
  "main": "dist/index.js",
  "scripts": {
    "test": "tsc --noEmit --project ./tsconfig.json",
    "build": "tsc --project ./tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WTW-IM/scriptloader-component.git"
  },
  "keywords": [
    "react",
    "component",
    "script",
    "onload"
  ],
  "author": "",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.(j|t)s?(x)": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.md": [
      "prettier --write --parser markdown"
    ]
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/WTW-IM/scriptloader-component/issues"
  },
  "homepage": "https://github.com/WTW-IM/scriptloader-component#readme",
  "peerDependencies": {
    "react": ">=16"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@types/node": "^14.14.9",
    "@types/react": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "conventional-changelog-eslint": "^3.0.9",
    "eslint": "^7.14.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.2",
    "prettier": "^2.2.0",
    "react": "^17.0.1",
    "semantic-release": "^17.3.0",
    "typescript": "^4.1.2"
  }
}
